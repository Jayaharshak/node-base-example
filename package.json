{
  "name": "node-seed-example",
  "version": "1.0.2",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "start": "pm2 start ./dist/index.js --watch",
    "prepublish": "npm run build",
    "build": "rimraf build && tsc -p tsconfig.json",
    "prepare": "tsc",
    "test": "jest",
    "test:unit": "jest -t=unit --coverage",
    "lint": "tslint --config ./tslint.json --project ./tsconfig.json --fix",
    "precommit": "concurrently \"npm run lint\" \"npm run test\""
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "jkandimalla",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "concurrently": "^6.3.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.2",
    "tslint": "^5.20.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "athena-express": "^7.1.0",
    "aws-sdk": "^2.1020.0",
    "encoding-utilities": "^1.0.1",
    "express": "^4.17.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
